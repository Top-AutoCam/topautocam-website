{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3||^8.2",
        "ajmariduena/dynamic-field": "^0.0.1",
        "ebess/advanced-nova-media-library": "^3.3",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "gregoriohc/laravel-nova-theme-responsive": "^0.8.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "hubertnnn/laravel-nova-field-dynamic-select": "^0.2.0",
        "laracademy/generators": "^2.3",
        "laravel/framework": "^8.0||^8.2",
        "laravel/nova": "*",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.2",
        "laravelcollective/html": "^6.2",
        "livewire/livewire": "^2.3",
        "lynndigital/select-or-custom": "dev-master",
        "optimistdigital/nova-blog": "^9.4",
        "optimistdigital/nova-drafts": "^1.1",
        "royduin/laravel-nova-field-dynamic-select": "^1.8",
        "spatie/laravel-permission": "^4.0"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0||^6.1",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ]
}
